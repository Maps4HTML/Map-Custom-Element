<mapml- xmlns="http://www.w3.org/1999/xhtml">
  <map-head>
    <meta charset="utf-8"/>
    <map-title>OpenStreetMap</map-title>
    <link rel="license" href="https://www.openstreetmap.org/copyright" title="Â© OpenStreetMap contributors CC BY-SA"/>
    <link rel="zoomout" href="cbmt-changeProjection.mapml" type="text/mapml" />
  </map-head>
  <map-body>
  <map-extent units="OSMTILE">
    <map-input name="z" type="zoom" min="9" max="18"></map-input>
    <map-input name="s" type="hidden" shard="true" list="servers"></map-input>
    <map-datalist id="servers">
      <option value="a"/>
      <option value="b"/>
      <option value="c"/>
    </map-datalist>
    <map-input name="x" type="location" units="tilematrix" axis="column"></map-input>
    <map-input name="y" type="location" units="tilematrix" axis="row"></map-input>
    <link rel="tile" tref="https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png"/>
  </map-extent>
  </map-body>
</mapml->
